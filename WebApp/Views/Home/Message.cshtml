@{ 
    Layout = "_SimpleLayout";
}

@if(Model is string)
{
    @Model;
}
else if(Model is IDictionary<string,string> dictionary)
{
    <table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Value</th>
            </tr>
        </thead>
        <tbody>
            @foreach(KeyValuePair<string,string> pair in dictionary)
            {
                <tr>
                    <td>@pair.Key</td>
                    <td>@pair.Value</td>
                </tr>
            }
        </tbody>
    </table>
}
